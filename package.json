{
  "name": "lumos-hub",
  "version": "1.0.0",
  "description": "Transform your study sessions with intelligent focus tools, beautiful progress tracking, and a supportive community",
  "type": "module",
  "homepage": "https://lumoshub.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/lumoshub/lumos-hub.git"
  },
  "bugs": {
    "url": "https://github.com/lumoshub/lumos-hub/issues"
  },
  "author": {
    "name": "Lumos Hub Team",
    "email": "team@lumoshub.com",
    "url": "https://lumoshub.com"
  },
  "license": "MIT",
  "keywords": [
    "study",
    "productivity",
    "pomodoro",
    "focus",
    "education",
    "learning",
    "timer",
    "wellness",
    "react",
    "vite"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rimraf dist node_modules/.vite",
    "postinstall": "patch-package",
    "deploy": "npm run build && npm run deploy:vercel",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "lighthouse": "lighthouse http://localhost:4173 --output json --output-path ./lighthouse-results.json",
    "size-limit": "size-limit",
    "update-deps": "ncu -u && npm install",
    "security-audit": "npm audit --audit-level moderate",
    "start": "npm run dev"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@supabase/supabase-js": "^2.56.1",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "focus-visible": "^5.2.1",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.279.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.46.1",
    "react-router-dom": "^6.30.1",
    "web-vitals": "^3.5.2",
    "workbox-window": "^7.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@eslint/js": "^8.49.0",
    "@playwright/test": "^1.38.1",
    "@size-limit/preset-app": "^8.2.6",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-react": "^4.0.4",
    "@webcomponents/custom-elements": "^1.6.0",
    "autoprefixer": "^10.4.15",
    "core-js": "^3.32.2",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "intersection-observer": "^0.12.2",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "npm-check-updates": "^16.14.5",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "regenerator-runtime": "^0.14.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "size-limit": "^8.2.6",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.16.5",
    "vitest": "^0.34.6",
    "web-animations-js": "^2.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist/assets/index-*.js",
      "limit": "500 KB"
    },
    {
      "path": "dist/assets/index-*.css",
      "limit": "50 KB"
    }
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "volta": {
    "node": "20.5.1",
    "npm": "9.8.0"
  }
}
